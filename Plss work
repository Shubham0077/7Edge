it('dispatches when both dropdowns are selected and Download is clicked', async () => {
  const { getByTestId } = render(<TdsPage />);

  const yearDropdown = getByTestId('year-dropdown');
  const quarterDropdown = getByTestId('quarter-dropdown');

  fireEvent.change(yearDropdown, { target: { value: '2023-2024' } });
  fireEvent.change(quarterDropdown, { target: { value: 'Q1' } });

  const downloadButton = getByTestId('download-button');
  fireEvent.click(downloadButton);

  await waitFor(() => {
    expect(mockDispatch).toHaveBeenCalled();
  });
});

