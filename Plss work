it('shows VerifyOtpModal when Download is clicked', async () => {
    const { getByTestId, getByText } = render(<TdsPage />);

    await user.selectOptions(getByTestId('year-dropdown'), '2023-2024');
    await user.selectOptions(getByTestId('quarter-dropdown'), 'Q1');

    await user.click(getByTestId('download-button'));

    await waitFor(() => {
      expect(getByText(/Verify via otp/i)).toBeInTheDocument();
    })
