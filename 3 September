const updatedCards = (
  tsMPST
    ? (() => {
        const cards = [
          ...(config?.section12?.cards ?? []),
          ...(config?.section17?.card ? [config.section17.card] : []),
        ];
        return cards.length ? cards : undefined; // keep undefined if empty
      })()
    : config?.section17?.cards
)?.map((card, index, arr) => ({
  ...card,
  isBottomDivider: index !== arr.length - 1,
}));
