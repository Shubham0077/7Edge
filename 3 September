const updatedCards = [
  ...(config?.section12?.cards ?? []),      // fallback to empty array
  ...(config?.section17?.cards ?? []),      // fallback to empty array
  ...(config?.section17?.card ? [config.section17.card] : []), // if single card exists
].map((card, index, arr) => ({
  ...card,
  isBottomDivider: index !== arr.length - 1, // true for all except the last one
}));
