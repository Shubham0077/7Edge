
export function getLast5FinancialYearsFromOldest(oldestDateStr: string): string[] {
  if (!oldestDateStr) return [];

  const currentYear = new Date().getFullYear();
  const oldestYear = new Date(oldestDateStr).getFullYear();

  const years = [];
  for (let year = oldestYear; year <= currentYear; year++) {
    years.push(`${year}-${year + 1}`);
  }

  // Return only last 5 years (from the end)
  return years.slice(-5);
}
