export function setLast5FinancialYearsFromOldest(
  oldestDateStr: string,
  setYears: React.Dispatch<React.SetStateAction<string[]>>
): void {
  if (!oldestDateStr) {
    setYears([]);
    return;
  }

  const currentYear = new Date().getFullYear();
  const oldestYear = new Date(oldestDateStr).getFullYear();

  const years: string[] = [];

  for (let year = oldestYear; year <= currentYear; year++) {
    years.push(`${year}-${year + 1}`);
  }

  setYears(years.slice(-5));
}



const [financialYears, setFinancialYears] = useState<{ label: string; value: string }[]>([]);
